{
  "rewrites": [
    {
      "source": "/auth/:path*",
      "destination": "https://kyradi-saas-canli-production.up.railway.app/auth/:path*"
    },
    {
      "source": "/admin/:path*",
      "destination": "https://kyradi-saas-canli-production.up.railway.app/admin/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
